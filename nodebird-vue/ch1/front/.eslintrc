{
    "parserOptions":{
        "ecamaVersion": 2019,
        "sourceType": "module" 
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "plugin:vue/recommended"
    ],
    "plugins": [],
    "rules": {
        "indent": ["error",2],
        "vue/html-indent": "off",
        "vue/max-attributes-per-line": "off",
        "vue/singleline-html-element-content-newline": "off"
    }
}